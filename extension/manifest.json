{
    "manifest_version": 3,
    "name": "Bezpieczna Strona",
    "version": "1.0",
    "description": "Sprawdza, czy aktualna strona jest bezpieczna.",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "16x16.png",
        "48": "48x48.png",
        "128": "128x128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  